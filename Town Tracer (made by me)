javascript:(function() {  if (navigator.geolocation) {    navigator.geolocation.getCurrentPosition(function(position) {      var latitude = position.coords.latitude;      var longitude = position.coords.longitude;      fetch(`https://nominatim.openstreetmap.org/reverse?format=jsonv2&lat=${latitude}&lon=${longitude}&zoom=10`)        .then(function(response) {          return response.json();        })        .then(function(data) {          var city = data.address.city || data.address.town || data.address.village || %27%27;          alert(%27 By WatermelonVR: Your location: %27 + city);        })        .catch(function(error) {          console.log(error);          alert(%27Failed to retrieve location data.%27);        });    });  } else {    alert(%27Geolocation is not supported by your browser.%27);  }})();
