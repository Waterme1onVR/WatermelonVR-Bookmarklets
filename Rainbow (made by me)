javascript:(function() {  var incrementingNum = 0;  var colors = "red orange yellow green blue purple pink".split(" ");  setInterval(function() {    if (incrementingNum >= 6) {      incrementingNum = 0;    } else {      incrementingNum++;    }    for (var i = 0; i < document.querySelectorAll("div").length; i++) {      var div = document.querySelectorAll("div")[i];      div.style.backgroundColor = colors[incrementingNum];      div.style.transition = "background-color 1s";    }    var watermark = document.createElement("div");    watermark.style.position = "fixed";    watermark.style.bottom = "30px";    watermark.style.left = "30px";    watermark.style.fontWeight = "bold";    watermark.style.color = "white";    watermark.textContent = "By WatermelonVR";    document.body.appendChild(watermark);  }, 1000);})();
