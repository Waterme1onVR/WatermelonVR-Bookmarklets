javascript:(function() {  var url = encodeURIComponent(window.location.href);  var qrCodeUrl = 'https://api.qrserver.com/v1/create-qr-code/?size=150x150&data=%27 + url;  var qrCodeContainer = document.createElement(%27div%27);  qrCodeContainer.style.position = %27fixed%27;  qrCodeContainer.style.left = %2710px%27;  qrCodeContainer.style.bottom = %2710px%27;  var qrCodeImage = document.createElement(%27img%27);  qrCodeImage.src = qrCodeUrl;  var watermark = document.createElement(%27div%27);  watermark.style.fontSize = %2720px%27;  watermark.style.fontFamily = %27Arial, sans-serif%27;  watermark.style.animation = %27colorCycle 5s infinite%27;  watermark.innerHTML = %27By WatermelonVR%27;  qrCodeContainer.appendChild(qrCodeImage);  qrCodeContainer.appendChild(watermark);  document.body.appendChild(qrCodeContainer);  var style = document.createElement(%27style%27);  style.innerHTML = `    @keyframes colorCycle {      0% { color: rgb(255, 0, 0); } /* Red */      25% { color: rgb(0, 255, 0); } /* Green */      50% { color: rgb(0, 0, 255); } /* Blue */      75% { color: rgb(255, 255, 0); } /* Yellow */      100% { color: rgb(255, 0, 255); } /* Magenta */    }  `;  document.head.appendChild(style);})();
