javascript:(function(x, y) {  if (!window.click) {    window.click = true;    document.body.style.cursor = 'crosshair';    var cps = prompt('Autoclicker CPS: (Under 200 recommended)');    if (!cps || isNaN(cps)) {      alert('You entered something wrong. Try running the script again.');      return;    }    alert('Autoclicker activated at ' + cps + ' CPS! Do [ctrl+e] to stop.');    var popup = document.createElement('div');    popup.innerHTML = '<span id="byText">By </span><span id="watermelonText">WatermelonVR</span>';    popup.style.position = 'fixed';    popup.style.left = '10px';    popup.style.bottom = '10px';    document.body.appendChild(popup);    var byTextElement = document.getElementById('byText');    var watermelonTextElement = document.getElementById('watermelonText');    var colors = ['red', 'orange', 'yellow', 'green', 'blue', 'indigo', 'violet'];    var currentIndex = 0;    setInterval(function() {      byTextElement.style.color = colors[currentIndex];      watermelonTextElement.style.color = colors[currentIndex];      currentIndex = (currentIndex + 1) % colors.length;    }, 500);    addEventListener('mousemove', function(e) {      x = e.clientX;      y = e.clientY;    });    addEventListener('keydown', function(e) {      if (e.key === 'e' && e.ctrlKey) {        alert('Autoclicker deactivated! Click the bookmark again to reactivate!');        end();      }    });    if (window.click) {      var int = setInterval(function() {        var element = document.elementFromPoint(x, y);        if (element) {          element.click();        }      }, 1000 / cps);    }    function end() {      clearInterval(int);      window.click = false;      document.body.style.cursor = 'default';      document.body.removeChild(popup);    }  }})();
