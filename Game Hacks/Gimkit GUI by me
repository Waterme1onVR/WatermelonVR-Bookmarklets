javascript:(function() {    var rectangle = document.createElement("div");    rectangle.id = "rectangle";    rectangle.style.width = "200px";    rectangle.style.height = "300px";    rectangle.style.backgroundColor = "black";    rectangle.style.position = "absolute";    rectangle.style.bottom = "0";    rectangle.style.left = "0";    rectangle.style.borderRadius = "10px";    var text = document.createElement("div");    text.textContent = "GimKit Cheat MenuV1.0";    text.style.color = "white";    text.style.fontFamily = "Arial, sans-serif";    text.style.fontSize = "18px";    text.style.textAlign = "center";    text.style.paddingTop = "10px";    var button1 = createButton("Flood");    var button2 = createButton("Auto-Answer");    var buttonContainer1 = document.createElement("div");    buttonContainer1.style.padding = "0 10px";    buttonContainer1.appendChild(button1);    buttonContainer1.appendChild(button2);    var button3 = createButton("Highlight Answer");    var button4 = createButton("Set Money");    var buttonContainer2 = document.createElement("div");    buttonContainer2.style.padding = "0 10px";    buttonContainer2.appendChild(button3);    buttonContainer2.appendChild(button4);    var bottomText = document.createElement("div");    bottomText.textContent = "WatermelonVR x Polar";    bottomText.style.color = "white";    bottomText.style.fontFamily = "Arial, sans-serif";    bottomText.style.fontSize = "14px";    bottomText.style.textAlign = "center";    bottomText.style.position = "absolute";    bottomText.style.bottom = "10px";    bottomText.style.left = "0";    bottomText.style.width = "100%";    rectangle.appendChild(text);    rectangle.appendChild(buttonContainer1);    rectangle.appendChild(buttonContainer2);    rectangle.appendChild(bottomText);    document.body.appendChild(rectangle);    var isDragging = false;    var mouseOffsetX = 0;    var mouseOffsetY = 0;    rectangle.addEventListener("mousedown", handleMouseDown);    rectangle.addEventListener("mouseup", handleMouseUp);    document.addEventListener("mousemove", handleMouseMove);    function handleMouseDown(event) {        isDragging = true;        var boundingRect = rectangle.getBoundingClientRect();        mouseOffsetX = event.clientX - boundingRect.left;        mouseOffsetY = event.clientY - boundingRect.top;    }    function handleMouseUp() {        isDragging = false;    }    function handleMouseMove(event) {        if (isDragging) {            rectangle.style.left = event.clientX - mouseOffsetX + "px";            rectangle.style.top = event.clientY - mouseOffsetY + "px";        }    }    function createButton(text) {        var button = document.createElement("button");        button.textContent = text;        button.style.width = "100%";        button.style.height = "30px";        button.style.backgroundColor = "grey";        button.style.color = "white";        button.style.fontFamily = "Arial, sans-serif";        button.style.fontSize = "14px";        button.style.margin = "5px 0";        button.style.border = "none";        button.style.borderRadius = "5px";        return button;    }    var isVisible = true;    var controlKeyPressed = false;    function handleKeyDown(e) {        if (e.key === "Control") {            controlKeyPressed = true;        } else if (e.key === "q" && controlKeyPressed) {            toggleMenuVisibility();        }    }    function handleKeyUp(e) {        if (e.key === "Control") {            controlKeyPressed = false;        }    }    function toggleMenuVisibility() {        isVisible = !isVisible;        rectangle.style.display = isVisible ? "block" : "none";    }    document.addEventListener("keydown", handleKeyDown);    document.addEventListener("keyup", handleKeyUp);})();
